<h3 style="display:inline">Status Feed</h3><br><br>
<ul class="nav nav-tabs">
<li><%= link_to "Friend's Feed", user_status_feed_path(current_user) %></li>
<li class="active"><%= link_to "My Activity", user_my_activity_path(current_user) %></li>
</ul>
<br><br>

<% if @microposts.count > 0 %>
	<% @microposts.each do |post|%>
	<p><%= post.content.html_safe + ' on ' + post.created_at %> </p> 
	<% end %>
<% else %>
	No updates from friends yet. <%= link_to "Find your friends", user_find_friends_path(current_user.id) %>
<% end %>